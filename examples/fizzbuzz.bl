  */


fun fizzbuzz(start, stop, step) {
	for (i = start; i <= stop; i += step) {
		if i % 15 == 0 {
			print('fizzbuzz');
		} else if i % 5 == 0 {
			print('buzz');
		} else if i % 3 == 0 {
			print('fizz');
		} else {
			print(i);
		}
	}
}

print('fizzbuzz program');
print('');
start = int(input('start from: '));
stop = int(input('to: '));
step = int(input('by: '));
print('');
fizzbuzz(start, stop, step);